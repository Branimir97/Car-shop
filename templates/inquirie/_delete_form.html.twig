<form method="post" action="{{ path('inquirie_delete', {'id': inquirie.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ inquirie.id) }}">
    <button class="btn">Delete</button>
</form>
